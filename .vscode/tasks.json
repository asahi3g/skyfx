{
    // See https://go.microsoft.com/fwlink/?LinkId=733558
    // for the documentation about the tasks.json format
    "options": {
        "env": {
          "PATH": "C:\\msys64\\usr\\bin;C:\\msys64\\mingw64\\bin"
        }
     },
    "version": "2.0.0",
    "tasks": [
        {
            "label": "DEBUG",
            "type": "shell",
            "command": "C:/msys64/usr/bin/make.exe",
            "args": [
                "debug",
            ],
            "problemMatcher": [
                "$gcc", "$go",
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "dependsOn": ["REMOVE_BIN_DEBUG"],
        },
        {
            "label": "RELEASE",
            "type": "shell",
            "command": "C:/msys64/usr/bin/make.exe",
            "args": [
                "release",
            ],
            "problemMatcher": [
                "$gcc", "$go",
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "dependsOn": ["REMOVE_BIN_DEBUG"],
        },
        {
            "label": "REMOVE_BIN_DEBUG",
            "type": "shell",
            "command": "C:/msys64/usr/bin/make.exe",
            "args": [
                "cleanbin",
                "BUILD=debug"
            ],
        },
        {
            "label": "REMOVE_BIN_RELEASE",
            "type": "shell",
            "command": "C:/msys64/usr/bin/make.exe",
            "args": [
                "cleanbin",
                "BUILD=release"
            ],
        }
    ]
}